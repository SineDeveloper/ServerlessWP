{
  "version": 2,
  "rewrites": [
    {
      "source": "/wp-content/uploads/(.*)",
      "destination": "/wp/wp-content/uploads/$1"
    },
    {
      "source": "/(wp-admin|wp-includes|wp-content)/(.*)",
      "destination": "/wp/$1/$2"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.php"
    }
  ]
}
