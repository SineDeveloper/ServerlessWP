{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "rewrites": [
    {
      "source": "/(.+)\\.(css|js|svg|png|gif|jpg|jpeg|webp|woff|woff2|ico|otf)",
      "destination": "/wp/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.php"
    }
  ]
}
