{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "rewrites": [
    {
      "source": "/wp-content/(.*)",
      "destination": "/wp/wp-content/$1"
    },
    {
      "source": "/wp-includes/(.*)",
      "destination": "/wp/wp-includes/$1"
    },
    {
      "source": "/wp-admin/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|otf))",
      "destination": "/wp/wp-admin/$1"
    },
    {
      "source": "/(.*\\.php)",
      "destination": "/api/index.php"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.php"
    }
  ]
}
