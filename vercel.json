{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "rewrites": [
    {
      "source": "/(wp-content|wp-includes)/(.*)",
      "destination": "/wp/$1/$2"
    },
    {
      "source": "/(.*\\.php)(.*)",
      "destination": "/api/index.php"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.php"
    }
  ]
}
